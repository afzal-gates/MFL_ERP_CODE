                <div class="row" style="padding-left: 5px; padding-right: 5px;">

                    <div class="row">
                        <div class="form-group">

                            <div class="col-md-1"></div>
                            <label class="control-label col-md-2">
                                <span aria-required="true" class="required">*</span>
                                Return Type
                            </label>
                            <div class="col-md-3">
                                <select kendo-drop-down-list name="ISS_RTN_TYPE_ID" id="ISS_RTN_TYPE_ID" class="form-control "
                                        options="vm.reqSourceList" ng-model="vm.form.ISS_RTN_TYPE_ID" tabindex="5" required></select>

                            </div>
                            <label class="col-md-2 control-label">
                                <span aria-required="true" class="required"></span>
                                Return By
                            </label>
                            <div class="col-md-2">
                                <select id="customers" kendo-drop-down-list options="vm.userList" k-ng-delay="vm.userList" ng-model="vm.form.ITEM_RET_BY" class="form-control required" style="width:100%;"></select>

                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group">
                            <div class="col-md-1">
                            </div>
