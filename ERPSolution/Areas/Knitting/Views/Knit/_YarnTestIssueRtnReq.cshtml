        <div class="row">
            <div class="col-md-4">
                <button type="button" class="btn btn-sm blue" ui-sref="YarnTestIssueRtnReqList">Back To List</button>
                <button type="button" ng-if="vm.form.KNT_YRN_ISS_RET_H_ID>0" ng-disabled="!vm.form.RF_ACTN_STATUS_ID>0" class="btn btn-sm yellow-gold" ng-click="vm.submitAll(vm.form);"> Submit</button>
                <button type="button" ng-if="!vm.form.KNT_YRN_ISS_RET_H_ID>0" ng-disabled="!vm.IssueItemList.length>0" class="btn btn-sm green" ng-click="vm.submitAll(vm.form);">Save</button>
                <button type="button" class="btn btn-sm default" ui-sref="YarnTestIssueRtnReq({pKNT_YRN_ISS_RET_H_ID : 0})" ui-sref-opts="{reload: true}">Cancel</button>
                &nbsp;&nbsp;&nbsp;&nbsp;
                @*<button type="button" ng-if="vm.form.KNT_YRN_ISS_RET_H_ID>0" class="btn btn-sm blue" ng-click="vm.printChallan()">Print Challan</button>*@
