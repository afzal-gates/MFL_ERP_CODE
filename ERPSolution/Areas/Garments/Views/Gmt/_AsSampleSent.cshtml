<div class="portlet box blue-dark">
    <div class="portlet-title">
        <div class="caption">
            {{vm.Title}}
        </div>
        <div class="tools">
        </div>

        <div class="actions">
        </div>
    </div>

    <div class="portlet-body">

        <table class="table table-condensed table-striped table-bordered" style="padding-left: 5px; padding-right: 5px; margin-bottom: 3px;">
            <tbody>
                <tr>
                    <td style="width: 100%; padding-bottom: 5px;" align="center">
                        <form name="JobCardSearchForm" ng-submit="vm.loadDataByJobCardNo(vm.JOB_CRD_NO)">
                            <div class="input-group input-medium">
                                <input type="text" class="form-control" ng-model="vm.JOB_CRD_NO" style="height:28px; width:200px;" focus-me="true" placeholder="Challan">
                                <span class="input-group-btn">
                                    <button class="btn btn-sm blue" type="submit">Go</button>
                                </span>
                                <span class="input-group-btn">
                                    <button class="btn btn-sm btn-default" ng-click="vm.gmtAsSampleSearchModal()"><i class="fa fa-search"></i> Search Challan For Edit</button>
                                </span>
                                &nbsp;
                                <span class="input-group-btn">
                                    <button class="btn btn-sm btn-primary"><i class=" fa fa-plus-circle"></i>  New Challan</button>
                                </span>

                            </div>
                        </form>
                    </td>
                </tr>
            </tbody>
        </table>


        <table class="table table-condensed table-striped borderless" style="margin-bottom:5px;">

            <thead>
                <tr style="border: 1px solid #cecece;">

                    <th style="width:10%">
                        Date:
                        <input type="text" class="form-control"  value="22-Jul-2018 " disabled/>
                    </th>


                    <th style="width:15%">
                        Buyer:
                        <select class="form-control" kendo-drop-down-list name="MC_DELV_WH_ID" k-option-label="'--Select --'"
                                k-data-text-field="'BUYER_NAME_EN'" k-data-value-field="'MC_DELV_WH_ID'"
                                k-auto-bind="'true'" k-data-source="vm.ProgramTypeDs" ng-model="form.MC_DELV_WH_ID"></select>
                    </th>

                    <th style="width:30%">
                        Style/Order
                        <select class="form-control" kendo-drop-down-list name="MC_DELV_WH_ID" k-option-label="'--Select --'"
                                k-data-text-field="'BUYER_NAME_EN'" k-data-value-field="'MC_DELV_WH_ID'"
                                k-auto-bind="'true'" k-data-source="vm.ProgramTypeDs" ng-model="form.MC_DELV_WH_ID"></select>
                    </th>
                    <th style="width:15%">
                        To: Section
                        <span class="col-md-10" style="padding-left:0px;padding-right:0px;">
                            <select class="form-control" kendo-drop-down-list name="MC_DELV_WH_ID" k-option-label="'Buyer QC Room'"
                                    k-data-text-field="'BUYER_NAME_EN'" k-data-value-field="'MC_DELV_WH_ID'"
                                    k-auto-bind="'true'" k-data-source="vm.DeliveryToListDs" ng-model="form.MC_DELV_WH_ID"></select>
                        </span>

                    </th>

                    <th style="width:10%">
                        From : Floor
                        <span class="col-md-10" style="padding-left:0px;padding-right:0px;">
                            <select class="form-control" kendo-drop-down-list name="MC_DELV_WH_ID" k-option-label="'Floor A'"
                                    k-data-text-field="'BUYER_NAME_EN'" k-data-value-field="'MC_DELV_WH_ID'"
                                    k-auto-bind="'true'" k-data-source="vm.DeliveryToListDs" ng-model="form.MC_DELV_WH_ID"></select>
                        </span>
                    </th>
                    <th style="width:10%">
                        From : Line
                        <span class="col-md-10" style="padding-left:0px;padding-right:0px;">
                            <select class="form-control" kendo-drop-down-list name="MC_DELV_WH_ID" k-option-label="'1'"
                                    k-data-text-field="'BUYER_NAME_EN'" k-data-value-field="'MC_DELV_WH_ID'"
                                    k-auto-bind="'true'" k-data-source="vm.DeliveryToListDs" ng-model="form.MC_DELV_WH_ID"></select>
                        </span>
                    </th>


                </tr>

            </thead>
        </table>

            <table class="table table-condensed table-striped table-bordered" style="padding:5px;">
                
                <thead>
                    <tr style="border: 1px solid #cecece">
                        <th style="width:30%;">
                            Buyer: Lindex>> Style: 21212 10001>> Order: 99999099999
                        </th>
                        <th style="width:7%;" rowspan="2">XS</th>
                        <th style="width:7%;" rowspan="2">S</th>
                        <th style="width:7%;" rowspan="2">M</th>
                        <th style="width:7%;" rowspan="2">L</th>
                        <th style="width:7%;" rowspan="2">XL</th>


                    </tr>

                    <tr style="border: 1px solid #cecece">
