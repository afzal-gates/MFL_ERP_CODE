                            <select kendo-drop-down-list id="RF_CURRENCY_ID" name="RF_CURRENCY_ID" class="form-control requiredSub"
                                    options="vm.currencyList" ng-model="form.RF_CURRENCY_ID"></select>
                        </div>

                    </div>
                </div>


