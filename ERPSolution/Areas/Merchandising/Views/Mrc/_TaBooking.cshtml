                            <select kendo-drop-down-list name="SCM_STORE_ID" id="SCM_STORE_ID" class="form-control required"
                                    options="vm.storeList" ng-model="form.SCM_STORE_ID" required></select>

                        </div>


                    </div>
                </div>

                <div class="row">
                    <div class="form-group">

                        <div class="col-md-1"></div>

                        <label class="col-md-2 control-label">
                            <span aria-required="true" class="required">*</span>
                            Priority
                        </label>
                        <div class="col-md-2">
                            <select kendo-drop-down-list name="LK_PRIORITY_ID" id="LK_PRIORITY_ID" class="form-control"
                                    options="vm.PriorityList" ng-model="form.LK_PRIORITY_ID" required></select>
