                            <span aria-required="true" class="required">*</span>
                            Company/Importer
                        </label>
                        <div class="col-md-3">
                            <select kendo-drop-down-list name="HR_COMPANY_ID" id="HR_COMPANY_ID" class="form-control required"
                                    ng-init="vm.form.HR_COMPANY_ID='@Session["multiLoginEmpCompId"]'" options="vm.companyList" ng-model="form.HR_COMPANY_ID" required></select>

                        </div>
                    </div>
                </div>


                <div class="row">
                    <div class="form-group">

                        <div class="col-md-1">
                        </div>
                        <label class="control-label col-md-2 ">
                            <span aria-required="true" class="required">*</span>
                            Requisitioned By
                        </label>
                        <div class="col-md-3">
                            <select id="PURC_REQ_BY" ng-init="form.PURC_REQ_BY='@Session["multiScUserId"]'" kendo-drop-down-list options="vm.userList" ng-model="form.PURC_REQ_BY" class="form-control customers" style="width:100%;"></select>
                        </div>

                        <label class="control-label col-md-2">
                            <span aria-required="true" class="required">*</span>
                            Requisition To
                        </label>
                        <div class="col-md-3">
                            <select id="PURC_REQ_TO" kendo-drop-down-list options="vm.userList" ng-model="form.PURC_REQ_TO" class="form-control customers required " style="width:100%;"></select>

                        </div>

                    </div>
                </div>



                <div class="row">
                    <div class="form-group">

                        <div class="col-md-1">
                        </div>
                        <label class="control-label col-md-2 ">
                            <span aria-required="true" class="required">*</span>
                            Action Status
                        </label>
                        <div class="col-md-3" ng-if="form.RF_ACTN_VIEW>0">
                            <select kendo-drop-down-list name="RF_ACTN_STATUS_ID" id="RF_ACTN_STATUS_ID" class="form-control"
                                    options="vm.statusList" ng-model="form.RF_ACTN_STATUS_ID" required></select>
                        </div>

                        <div class="col-md-3" ng-if="!form.RF_ACTN_VIEW>0">

                            <input type="text" ng-model="form.ACTN_STATUS_NAME" style="width: 100%;" readonly="readonly" />
                            <input type="hidden" ng-model="form.RF_ACTN_STATUS_ID" />

                        </div>


                    </div>
                </div>
                
            </fieldset>

        </form>
        <br />

        <div class="row" style="margin:auto;">
            <tabset>
                <tab ng-repeat="tab in tabs" deselect="tab.IS_TAB_ACT=false" select="tab.IS_TAB_ACT=true" heading="{{tab.ITEM_NAME_EN}}" active="tab.IS_TAB_ACT">
                    <div data-ui-view="TabInside{{tab.IS_TAB_ACT?'':0}}"></div>
                </tab>
            </tabset>
        </div>
    </div>




