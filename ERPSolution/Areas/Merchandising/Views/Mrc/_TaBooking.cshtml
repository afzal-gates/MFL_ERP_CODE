                <div class="row">
                    <div class="form-group">
                        <div class="col-md-1">
                        </div>
                        <label class="control-label col-md-2">
                            <span aria-required="true" class="required">*</span>
                            Requisition Type
                        </label>
                        <div class="col-md-3">
                            <select kendo-drop-down-list name="RF_REQ_TYPE_ID" id="RF_REQ_TYPE_ID" class="form-control required"
                                    options="vm.reqTypeList" ng-model="form.RF_REQ_TYPE_ID" ng-init="form.RF_REQ_TYPE_ID=30" required></select>

                        </div>

                        <label class="control-label col-md-2">
                            <span aria-required="true" class="required">*</span>
                            Sourcing Type
                        </label>
                        <div class="col-md-3">

                            <select kendo-drop-down-list name="LK_LOC_SRC_TYPE_ID" id="LK_LOC_SRC_TYPE_ID" class="form-control"
                                    options="vm.sourceTypeList" ng-model="form.LK_LOC_SRC_TYPE_ID"></select>

                        </div>
                    </div>
                </div>


                <div class="row">
                    <div class="form-group">
                        <div class="col-md-1">
                        </div>
                        <label class="control-label col-md-2">
                            <span aria-required="true" class="required"></span>
                            Requisition No
                        </label>
                        <div class="col-md-2">
                            <input type="text" name="PURC_REQ_NO" placeholder="[Auto Gen]" ng-model="form.PURC_REQ_NO" readonly="readonly" class="form-control" />
                            <input type="hidden" id="SCM_PURC_REQ_H_ID" name="SCM_PURC_REQ_H_ID" ng-model="form.SCM_PURC_REQ_H_ID" />
                        </div>

                        <div class="col-md-1">
                        </div>
                        <label class="col-md-2 control-label">
                            <span aria-required="true" class="required">*</span>
                            Date of Requisition
                        </label>
                        <div class="col-md-2">
                            <input type="text" class="form-control" ng-init="form.PURC_REQ_DT=vm.today" ng-click="vm.ACCS_DELV_DT_OPEN($event)" datepicker-popup="{{vm.dtFormat}}"
                                   name="PURC_REQ_DT" ng-model="form.PURC_REQ_DT" is-open="vm.ACCS_DELV_DT_OPENED" datepicker-options="dateOptions" date-disabled="disabled(date, mode)" required close-text="Close" />

                            @*<input type="text" class="form-control required" ng-click="PURC_REQ_DT_LNopen($event)" datepicker-popup="{{vm.dtFormat}}"
