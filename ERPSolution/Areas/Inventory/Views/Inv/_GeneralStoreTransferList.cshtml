<div class="portlet box blue-dark">
    <div class="portlet-title">
        <div class="caption" ng-bind="vm.Title">
        </div>
        <div class="tools">
        </div>
    </div>

    <div class="portlet-body">

        <div id="splash-page" data-ng-show="vm.showSplash" class="dissolve-animation">
            <div class="page-splash">
                <div class="page-splash-message">
                    <img src="~/Content/assets/global/img/ajax-loading.gif" />  <b>Please wait ....</b>
                </div>
            </div>
        </div>
        <input id="access_token" data-ng-model="token" type="hidden"
               data-ng-init="token='@Session["access_token"]'" />
        <input ng-model="vm.form.CREATED_BY" type="hidden"
               data-ng-init="vm.form.CREATED_BY='@Session["multiScUserId"]'" />

        <input id="access_token" ng-model="vm.form.LAST_UPDATED_BY" type="hidden"
               ng-init="vm.form.LAST_UPDATED_BY='@Session["multiScUserId"]'" />

        <div class="row" style="padding:5px">
            <div class="form-group" >
                <div class="col-md-2">
                    <a ui-sref="GeneralStoreTransfer({pINV_STR_TR_REQ_H_ID : 0, pLK_STR_TRN_TYP_ID:vm.LK_STR_TRN_TYP_ID})" class="btn btn-sm blue">
                        <i class="fa fa-plus"></i> Add New
                    </a>
                </div>
                <div class="col-md-8">

                    <input type="radio" id="LK_STR_TRN_TYP_ID_0" name="LK_STR_TRN_TYP_ID" ng-model="vm.LK_STR_TRN_TYP_ID" value="643"><label for="LK_STR_TRN_TYP_ID_0"> <b>Main to Main</b>&nbsp; &nbsp;</label><input type="radio" id="LK_STR_TRN_TYP_ID_1" name="LK_STR_TRN_TYP_ID" ng-model="vm.LK_STR_TRN_TYP_ID" value="644"><label for="LK_STR_TRN_TYP_ID_1"><b> Main to Sub</b>&nbsp; &nbsp;</label><input type="radio" name="LK_STR_TRN_TYP_ID" id="LK_STR_TRN_TYP_ID_2" ng-model="vm.LK_STR_TRN_TYP_ID" value="645"><label for="LK_STR_TRN_TYP_ID_2"><b>Sub to Sub</b> </label>
                </div>
            </div>
        </div>

        <div style="padding:5px;">
            <kendo-grid k-options="vm.gridOptions">
                <div k-detail-template>
                    <div>
                        <div kendo-grid k-options="vm.detailGridOptionsForST(dataItem)"></div>
                    </div>
                </div>
            </kendo-grid>
        </div>
    </div>
</div>
