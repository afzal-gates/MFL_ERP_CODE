<script>

    function changePhoto(input) {
        var empCode = document.getElementsByName('EMPLOYEE_CODE');
        
        if (empCode[0].value == null || empCode[0].value=='') {
            alert('Sorry! Please input employee code')
        }
        else {
            readPhoto(input);
        }        
    };

    function readPhoto(input) {
        //alert('test');
        if (input.files && input.files[0]) {
            var reader = new FileReader();

            reader.onload = function (e) {
                $('#empPhoto').attr('src', e.target.result);
            }

            reader.readAsDataURL(input.files[0]);
        }
    };

    function empPhotoLoad(p, f) {
        alert('test');
        $('#empPhoto').attr('src', '~/UPLOAD_DOCS/EMP_PHOTOS/)'+f+'.jpg');
    }

    //$("#ATT_FILE").change(function () {
    //    alert('df');
    //    readURL(this);
    //});

    function UploadPic() {        
        var data = new FormData();

        var files = $("#ATT_FILE").get(0).files;

        // Add the uploaded image content to the form data collection
        if (files.length > 0) {
            data.append("UploadedImage", files[0]);
        }

        $.ajax({ 
            method: 'post',
            url: '/Hr/HrEmployee/UploadFiles',
            contentType: false,
            processData: false,
            data: data
        });
        
    }
    
    
</script>


