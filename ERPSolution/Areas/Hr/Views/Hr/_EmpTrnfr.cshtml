<div class="portlet box blue-dark">
    <div class="portlet-title">
        <div class="caption" @*ng-bind="vm.Title"*@>
            {{vm.Title}}@*&nbsp;<span style="color:#ff4600" ng-show="form.HR_YR_INCR_H_ID>0"><b>[ {{form.BATCH_APVRL_STATUS}} ]</b></span>*@
        </div>
        <div class="tools">
        </div>
    </div>


    <div class="portlet-body form">
        <div id="splash-page" data-ng-show="vm.showSplash" class="dissolve-animation">
            <div class="page-splash">
                <div class="page-splash-message">
                    <img src="~/Content/assets/global/img/ajax-loading.gif" />  <b>Please wait ....</b>
                </div>
            </div>
        </div>

        <form class="form-horizontal" role="form" name="frmEmpTransfer" novalidate>
            <div class="form-body clearfix">
                <div class="row" style="margin:auto">

                    <div class="row" style="margin:auto;" ng-show="vm.errors">
                        <div class="alert alert-danger" role="alert">
                            @*<button type="button" class="close" data-dismiss="alert" aria-hidden="true"></button>*@
                            <div style="padding: 0px 0px 15px 25px; ">
                                <b>Please fix the following errors ....</b>
                            </div>
                            <ul ng-repeat="field in vm.errors">
                                <li ng-repeat="error in field" ng-bind="error"></li>
                            </ul>
                        </div>
                    </div>


                    <div class="col-md-6" style="padding-left:0px">
                        <fieldset class="custom-fieldset">
                            <legend class="custom-legend">Previous Information</legend>

                            <div class="form-group">
                                <label class="control-label col-md-3">
                                    <span aria-required="true" class="required">*</span>Code
                                </label>
                                <div class="col-md-4">
                                    <input type="text" id="frmEMPLOYEE_CODE" ng-model="vm.form.P_EMPLOYEE_CODE" placeholder="Search..." @*ng-disabled="vm.isEmployee!=true"*@
                                           typeahead="item.EMPLOYEE_CODE as item.EMPLOYEE_CODE for item in emoloyeeAuto($viewValue)"
                                           typeahead-min-length="3" typeahead-wait-ms="1000"
                                           typeahead-on-select="onSelectItem($item)" typeahead-loading="loadingLocationsEmp"
                                           class="form-control" ng-disabled="vm.form.HR_EMP_TRNFR_ID>0" autocomplete="off" />
                                    <i ng-show="loadingLocationsEmp" class="glyphicon glyphicon-refresh"></i>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3">
                                    Name
                                </label>
                                <div class="col-md-8">
                                    <input type="text" class="form-control" name="EMP_FULL_NAME_EN" ng-model="vm.form.EMP_FULL_NAME_EN" readonly />
                                    @*<textarea class="form-control" ng-model="vm.form.EMP_PROFILE" rows="3" ng-disabled="true"></textarea>*@
                                </div>

                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3">
                                    Company
                                </label>
                                <div class="col-md-8">
                                    <input type="text" class="form-control" name="P_COMP_NAME_EN" ng-model="vm.form.P_COMP_NAME_EN" readonly />
                                </div>

                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3">
                                    Office
                                </label>
                                <div class="col-md-8">
                                    <input type="text" class="form-control" name="P_OFFICE_NAME_EN" ng-model="vm.form.P_OFFICE_NAME_EN" readonly />
                                </div>

                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3">
                                    Section
                                </label>
                                <div class="col-md-8">
                                    <input type="text" class="form-control" name="P_DEPARTMENT_NAME_EN" ng-model="vm.form.P_DEPARTMENT_NAME_EN" readonly />
                                </div>

                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3">
                                    Designation
                                </label>
                                <div class="col-md-8">
                                    <input type="text" class="form-control" name="P_DESIGNATION_NAME_EN" ng-model="vm.form.P_DESIGNATION_NAME_EN" readonly />
                                </div>

                            </div>


                            <div class="form-group">
                                <label class="control-label col-md-3">
                                    Floor & Line
                                </label>
                                <div class="col-md-5">
                                    <input type="text" class="form-control" name="P_FLOOR_DESC_EN" ng-model="vm.form.P_FLOOR_DESC_EN" readonly />
                                </div>
                                <div class="col-md-3">
                                    <input type="text" class="form-control" name="P_LINE_NO" ng-model="vm.form.P_LINE_NO" readonly />
                                </div>

                            </div>

                        </fieldset>
                    </div>

                    
                    <div class="col-md-6" style="padding-left:0px;padding-right:0px;">
                        <fieldset class="custom-fieldset">
                            <legend class="custom-legend">New Information</legend>

                            @*<div class="form-group">
                                <label class="control-label col-md-3">
                                    Ref#
                                </label>
                                <div class="col-md-4">
                                    <input type="text" class="form-control" ng-model="vm.form.TRNF_REF_NO" readonly />
                                </div>
                            </div>*@

                            <div class="form-group">
                                <label class="control-label col-md-3">
                                    Ref Date
                                </label>
                                <div class="col-md-3">
                                    <input type="text" class="form-control" ng-click="vm.trnfRefDateOpen($event)" datepicker-popup="{{vm.dtFormat}}" name="TRNF_REF_DT" ng-model="vm.form.TRNF_REF_DT"
                                           is-open="vm.trnfRefDateOpened" datepicker-options="dateOptions" date-disabled="disabled(date, mode)" @*ng-required="true"*@ close-text="Close" required />
                                </div>

                                <label class="control-label col-md-2">
                                    Ref#
                                </label>
                                <div class="col-md-3">
                                    <input type="text" class="form-control" ng-model="vm.form.TRNF_REF_NO" readonly />
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3">
                                    Effect Date
                                </label>
                                <div class="col-md-3">
                                    <input type="text" class="form-control" ng-click="vm.effectDateOpen($event)" datepicker-popup="{{vm.dtFormat}}" name="EFFECT_DT" ng-model="vm.form.EFFECT_DT"
                                           is-open="vm.effectDateOpened" datepicker-options="dateOptions" date-disabled="disabled(date, mode)" @*ng-required="true"*@ close-text="Close" required />
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3">
                                    Company
                                </label>
                                <div class="col-md-8">
                                    <select class="form-control" kendo-drop-down-list name="N_HR_COMPANY_ID" required
                                            k-data-text-field="'COMP_NAME_EN'" k-data-value-field="'HR_COMPANY_ID'" k-option-label="'-- Select --'"
                                            k-auto-bind="'true'" k-options="vm.compOptions" ng-model="vm.form.N_HR_COMPANY_ID"
                                            @*k-on-select="vm.onSelectCompany(kendoEvent)"*@></select>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3">
                                    Office
                                </label>
                                <div class="col-md-8">
                                    <select class="form-control" kendo-drop-down-list name="N_HR_OFFICE_ID" required
                                            k-data-text-field="'OFFICE_NAME_EN'" k-data-value-field="'HR_OFFICE_ID'" k-option-label="'-- Select --'"
                                            k-auto-bind="'true'" k-options="vm.officeOption" k-data-source="vm.officeDataSource" ng-model="vm.form.N_HR_OFFICE_ID"
                                            @*k-on-select="vm.onSelectCompany(kendoEvent)"*@></select>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3">
                                    Section
                                </label>
                                <div class="col-md-8">
                                    <select kendo-drop-down-list id="N_HR_DEPARTMENT_ID" name="N_HR_DEPARTMENT_ID" class="form-control" @*ng-disabled="vm.isManagementType!=true"*@
                                            options="vm.sectionOption" ng-model="vm.form.N_HR_DEPARTMENT_ID" required></select>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3">
                                    Designation
                                </label>
                                <div class="col-md-8">
                                    <select kendo-drop-down-list id="N_HR_DESIGNATION_ID" name="N_HR_DESIGNATION_ID" class="form-control" @*ng-disabled="vm.isManagementType!=true"*@
                                            options="vm.desigOption" k-data-source="vm.desigDataSource" ng-model="vm.form.N_HR_DESIGNATION_ID" required></select>
                                </div>
                            </div>

                            @*<div class="form-group">
                                <label class="control-label col-md-3">
                                    Floor & Line
                                </label>
                                <div class="col-md-5">
                                    <select class="form-control" kendo-drop-down-list name="N_HR_PROD_FLR_ID" required
                                            k-data-text-field="'FLOOR_DESC_EN'" k-data-value-field="'HR_PROD_FLR_ID'" k-option-label="'-- Select --'"
                                            k-auto-bind="'true'" k-options="vm.prodFloorOption" k-data-source="vm.prodFloorDataSource" ng-model="vm.form.N_HR_PROD_FLR_ID" ></select>
                                </div>

                                <div class="col-md-3">
                                    <select class="form-control" kendo-drop-down-list name="N_HR_PROD_FLR_ID" required
                                            k-data-text-field="'LINE_NO'" k-data-value-field="'HR_PROD_LINE_ID'" k-option-label="'-- Select --'"
                                            k-auto-bind="'true'" k-options="vm.prodLineOption" k-data-source="vm.prodLineDataSource" ng-model="vm.form.N_HR_PROD_LINE_ID"></select>
                                </div>
                            </div>*@

                            <div class="form-group">
                                <label class="control-label col-md-3">
                                    Floor & Line
                                </label>
                                <div class="col-md-5">
                                    <select class="form-control" kendo-drop-down-list name="N_LK_FLOOR_ID"                                             
                                            k-auto-bind="'true'" k-options="vm.lkFloorOption" ng-model="vm.form.N_LK_FLOOR_ID"></select>
                                </div>
                                <div class="col-md-3">
                                    <select class="form-control" kendo-drop-down-list name="N_LINE_NO"                                             
                                            k-auto-bind="'true'" k-options="vm.lkLineOption" ng-model="vm.form.N_LINE_NO"></select>
                                </div>

                            </div>

                            <div class="form-group">
                                <div class="col-md-offset-3 col-md-10">
                                    <button type="button" class="btn green btn-sm" ng-click="vm.save(vm.form,'N')" ng-class="{'disabled':!frmEmpTransfer.$valid}"
                                            ng-show="vm.form.HR_EMP_TRNFR_ID<1" ng-disabled="vm.form.IS_FINALIZED=='Y'">
                                        Save
                                    </button>
                                    <button type="button" class="btn yellow-gold btn-sm" ng-click="vm.save(vm.form,'N')" ng-class="{'disabled':!frmEmpTransfer.$valid}"
                                            ng-show="vm.form.HR_EMP_TRNFR_ID>0" ng-disabled="vm.form.IS_FINALIZED=='Y'">
                                        Update
                                    </button>

                                    <button type="button" class="btn btn-default btn-sm" ng-click="vm.tranCancel()">Cancel</button>

                                    @*<button type="button" class="btn btn-default btn-sm" ng-click="vm.tranCancel()">View History</button>*@
                                </div>
                            </div>

                        </fieldset>
                    </div>

                    
                    @*<pre>{{vm.form|json}}</pre>*@
                    
                    
                    @*<div>&nbsp;</div>*@

                    <div class="col-md-12" style="padding-top:5px;padding-left:0px;padding-right:0px">
                        <fieldset class="custom-fieldset" style="padding-top:5px;padding-bottom:5px;">
                            <legend class="custom-legend">Transfer List</legend>

                            <kendo-grid k-options="vm.transGridOption" k-data-source="vm.transGridDataSource"></kendo-grid>

                        </fieldset>
                    </div>

                    

                                 
                </div>

                @*<pre>{{vm.form|json}}</pre>*@
                @*<pre>{{form|json}}</pre>*@
                
             
                @*<div class="portlet-body" data-ui-view="IncrimentProposalH@Dtl" ></div>*@
               
            </div>
        </form>



    </div>

</div>






