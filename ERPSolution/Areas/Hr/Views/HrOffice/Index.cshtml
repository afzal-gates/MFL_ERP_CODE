@{
    ViewBag.Title = "Office/Unit Information";
}

<div class="portlet box blue-dark" ng-controller="HrOfficeController as vm">
    @* Start Title *@
    <div class="portlet-title">
        <div class="caption">
            @*<i class="fa fa-gift"></i>*@ Office/Unit Information
        </div>
        <div class="tools">
            @*<a href="javascript:;" class="collapse">
                </a>*@
            @*<a href="#portlet-config" data-toggle="modal" class="config">
                </a>
                <a href="javascript:;" class="reload">
                </a>*@
            @*<a href="javascript:;" class="remove">
                </a>*@
        </div>
    </div>
    @* End Title *@


    @* Start Form portlet-body *@
    <div class="portlet-body form">


        <div class="row">

            <div class="col-md-12">
                @*<div data-ui-view class="shuffle-animation"></div>*@


                <tabset>

                    <tab ng-repeat="tab in vm.tabs" heading="{{tab.title}}" active="tab.active" ui-sref="{{tab.url}}" disabled="tab.disabled">
                        <div data-ui-view  class="shuffle-animation"></div>
                    </tab>

                </tabset>

            </div>

        </div>
    </div>
    @* End Form portlet-body *@



</div>


