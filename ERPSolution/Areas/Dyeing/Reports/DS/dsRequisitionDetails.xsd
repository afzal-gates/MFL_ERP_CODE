                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 
                STR_REQ_NO, STR_REQ_DT, LOAD_TIME, 
                UN_LOAD_TIME, REMARKS, RQD_QTY, RD.RQD_QTY_K, RD.RQD_QTY_G, RD.RQD_QTY_M,
