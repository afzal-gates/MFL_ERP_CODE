                <button type="button" ng-if="vm.form.DF_SC_PT_ISS_H_ID>0" class="btn btn-sm yellow-gold" ng-disabled="!vm.form.RF_ACTN_STATUS_ID>0" ng-click="vm.submitAll(vm.form,'N');"> Submit</button>
                <button type="button" ng-if="vm.form.DF_SC_PT_ISS_H_ID>0" class="btn btn-sm yellow-lemon" ng-disabled="!vm.form.RF_ACTN_STATUS_ID>0" ng-click="vm.submitAll(vm.form,'Y');"> Update</button>
                <button type="button" ng-if="vm.form.DF_SC_PT_ISS_H_ID>0" class="btn btn-sm green" ng-click="vm.printRDLC(vm.form);"> Print</button>
                <button type="button" ng-if="!vm.form.DF_SC_PT_ISS_H_ID>0" class="btn btn-sm green" ng-disabled="!vm.form.RF_ACTN_STATUS_ID>0" ng-click="vm.submitAll(vm.form,'N');">Save</button>
                <button type="button" class="btn btn-sm default" ui-sref="DyeingScProgram({pDF_SC_PT_ISS_H_ID : 0})" ui-sref-opts="{reload: true}">Cancel</button>

            </div>

            <label class="control-label col-md-2">
                <span aria-required="true" class="required">*</span>
                Take Action
            </label>
            <div class="col-md-3" ng-if="vm.form.RF_ACTN_VIEW>0">
                <select kendo-drop-down-list name="RF_ACTN_STATUS_ID" id="RF_ACTN_STATUS_ID" class="form-control required"
                        options="vm.statusList" ng-model="vm.form.RF_ACTN_STATUS_ID" required></select>
            </div>

            <div class="col-md-3" ng-if="!vm.form.RF_ACTN_VIEW>0">

                <input type="text" ng-model="vm.form.ACTN_STATUS_NAME" style="width: 100%;" readonly="readonly" />
                <input type="hidden" ng-model="vm.form.RF_ACTN_STATUS_ID" />

            </div>

        </div>
        <div style="padding:3px;">
        </div>
    </form>
</div>
