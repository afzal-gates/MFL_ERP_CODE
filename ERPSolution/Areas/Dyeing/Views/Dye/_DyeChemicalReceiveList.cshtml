<ul class="page-breadcrumb breadcrumb">
    <li>
        <a href="index.html">MultiTex ERP</a>

    </li>
    <li>
        <a href="#">Dyeing</a>

    </li>
    <li>
        <a href="#">Dyes Chemical Receive List</a>
    </li>
</ul>



<div class="portlet box blue-dark">
    <div class="portlet-title">
        <div class="caption" ng-bind="vm.Title">
        </div>
        <div class="tools">
        </div>
    </div>

    <div class="portlet-body">

        <div id="splash-page" data-ng-show="vm.showSplash" class="dissolve-animation">
            <div class="page-splash">
                <div class="page-splash-message">
                    <img src="~/Content/assets/global/img/ajax-loading.gif" />  <b>Please wait ....</b>
                </div>
            </div>
        </div>
        <input id="access_token" data-ng-model="token" type="hidden"
               data-ng-init="token='@Session["access_token"]'" />
        <input ng-model="vm.form.CREATED_BY" type="hidden"
               data-ng-init="vm.form.CREATED_BY='@Session["multiScUserId"]'" />

        <input id="access_token" ng-model="vm.form.LAST_UPDATED_BY" type="hidden"
               ng-init="vm.form.LAST_UPDATED_BY='@Session["multiScUserId"]'" />

        <div class="row" style="padding:5px">
            <div class="col-md-1">
                <a ui-sref="DyeChemicalReceive({pDYE_DC_RCV_H_ID : 0})" class="btn btn-sm blue">
                    <i class="fa fa-plus"></i> Add New
                </a>
            </div>
        </div>

        <div style="padding:5px;">

            <fieldset class="custom-fieldset">
                <legend class="custom-legend"><b>Searching Data</b></legend>
                <table class="clsTable">
                    <thead class="clsthead">
                        <tr>
                            <th class="clstd">
                                <span aria-required="true" class="required"></span>
                                MRR No
                            </th>
                            <th class="clstd">
                                <span aria-required="true" class="required"></span>
                                Date
                            </th>
                            <th class="clstd" style="width:150px;">
                                <span aria-required="true" class="required"></span>
                                Company
                            </th>
                            <th class="clstd">
                                <span aria-required="true" class="required"></span>
                                Pay Method
                            </th>
                            <th class="clstd">
                                <span aria-required="true" class="required"></span>
                                Source Type
                            </th>
                            <th class="clstd">
                                <span aria-required="true" class="required"></span>
                                Challan No
                            </th>
                            <th class="clstd">
                                <span aria-required="true" class="required"></span>
                                L/C No
                            </th>

                            <th class="clstd">
                                <span aria-required="true" class="required"></span>
                                Action
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr class="clstr">
                            <td class="clstd">
                                <input type="text" name="DC_MRR_NO" ng-model="vm.form.DC_MRR_NO" class="form-control" />
                            </td>
                            <td class="clstd">
                                <input type="text" class="form-control" ng-click="DC_MRR_DT_LNopen($event)" datepicker-popup="{{vm.dtFormat}}"
                                       name="DC_MRR_DT" max-date="vm.today" ng-model="vm.form.DC_MRR_DT" is-open="DC_MRR_DT_LNopened"
                                       datepicker-options="dateOptions" date-disabled="disabled(date, mode)" close-text="Close" />

                            </td>
                            <td class="clstd" style="width:150px;">

                                <input type="text" name="COMP_NAME_EN" ng-model="vm.form.COMP_NAME_EN" class="form-control" />
                            </td>
                            <td class="clstd">

                                <input type="text" name="PAY_MTHD_NAME" ng-model="vm.form.PAY_MTHD_NAME" class="form-control" />

                            </td>

                            <td class="clstd">
                                <input type="text" name="LOC_SRC_TYPE_NAME" ng-model="vm.form.LOC_SRC_TYPE_NAME" class="form-control" />
                            </td>

                            <td class="clstd">
                                <input type="text" name="CHALAN_NO" ng-model="vm.form.CHALAN_NO" class="form-control" />
                            </td>

                            <td class="clstd">
                                <input type="text" name="IMP_LC_NO" ng-model="vm.form.IMP_LC_NO" class="form-control" />
                            </td>


                            <td class="clstd">
                                <button type="button" class="btn btn-sm default" ng-click="vm.searchData()">Search</button>
                            </td>

                        </tr>
                    </tbody>
                </table>
            </fieldset>


            <kendo-grid id="grid" k-options="vm.gridOptions" k-data-source="vm.gridOptionsDS"></kendo-grid>
        </div>
    </div>
</div>
