        </div>
        
        <div class="col-md-7">



            <div class="input-icon right" style="padding-bottom:3px;">
                <i class="icon-magnifier"></i>
                <input type="text" class="form-control form-control-static" ng-model="q2" placeholder="Search by Buyer/Style/Order/Lines/Basic/Polo/Fancy/No Plan/Projected/ShipOut/Data Missing" ng-change="item.order_lists_ds.filter({logic: 'or', filters:[{ field:'STYLE_NO', operator :'contains', value: q2 }, { field:'BYR_ACC_NAME_EN', operator :'contains', value: q2 }, { field:'ORDER_NO', operator :'contains', value: q2 },{ field:'DATA_STATUS', operator :'contains', value: q2 },{ field:'PLAN_STATUS', operator :'contains', value: q2 }, { field:'LK_DATA_NAME_EN', operator :'contains', value: q2 }, { field:'LINE_CODE_LST', operator :'contains', value: q2 }]})" ng-model-options="{ debounce: { 'default': 1000, 'blur': 0 } }">
            </div>


            <kendo-grid k-options="vm.OrderListOpt" k-data-source="item.order_lists_ds"></kendo-grid>

