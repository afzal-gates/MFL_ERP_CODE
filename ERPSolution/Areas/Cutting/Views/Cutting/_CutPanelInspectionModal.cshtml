                                    <table class="table table-condensed table-responsive table-bordered" style="padding: 2px; margin-bottom: 2px;">
                                        <tbody>
                                            <tr>
                                                <td style="padding-bottom: 5px; padding-top: 0px;" ng-repeat="item in CurrentBundle.defects|limitTo :4:0">
                                                    <b>{{item.DISPLAY_ORDER}}. {{item.FB_DFCT_TYPE_NAME}}</b>
                                                    <div class="input-group" style="padding-left: 15px; padding-right: 15px;">
                                                        <span class="input-group-btn">
                                                            <button class="btn  red" ng-disabled="item.NO_OF_DFCT==0" ng-click="addOne(item,-1);" type="button"><b> &nbsp; &nbsp; &nbsp; - &nbsp; &nbsp; &nbsp;</b></button>
                                                        </span>
                                                        <input type="number" ng-model="item.NO_OF_DFCT" ng-click="setSelected(item, CurrentBundle.defects)" ng-style="{'background-color': item.SELECTED ?'blue':'white','color': item.SELECTED ?'white':'','color': item.SELECTED ?'white':'', 'color': item.NO_OF_DFCT > 0 ? '#E87E04':'', 'font-weight': item.NO_OF_DFCT > 0 ? '800':'600'  }" style="height: 35px; font-size: 20px;text-align:center;font-weight: 600;" readonly min="0" class="form-control">
                                                        <span class="input-group-btn">
                                                            <button class="btn   green" ng-disabled="item.NO_OF_DFCT==99" ng-click="addOne(item,1);setSelected(item, CurrentBundle.defects);" type="button"><b> &nbsp; &nbsp; &nbsp; + &nbsp; &nbsp; &nbsp;</b></button>
                                                        </span>
                                                    </div>
                                                </td>
                                           </tr>

                                            <tr>
                                                <td style="padding-bottom: 5px; padding-top: 0px;" ng-repeat="item in CurrentBundle.defects|limitTo :4:4">
