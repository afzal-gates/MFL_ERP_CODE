                                            <tr>

                                                <td>
                                                    <br />
                                                    <br />
                                                    <br />

                                 
                                                </td>

                                            </tr>




                                            <tr>

                                                <td style="padding-bottom: 5px; padding-top: 0px;">
                                                    <h1 style="font-size: 40px;font-weight:900">{{CurrentBundle.NO_OF_PANEL_REJ||0}}</h1>   Panel Reject
                                                </td>

                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </td>
                        </tr>

                    </tbody>
                    <tfoot>
                        <tr>
                            <td>
                                <div class="btn-group btn-group-justified">
                      
                                    <a class="btn btn-lg yellow-gold" ng-click="selectedDefect.NO_OF_DFCT > 99? selectedDefect.NO_OF_DFCT: selectedDefect.NO_OF_DFCT=(selectedDefect.NO_OF_DFCT+'1')*1;setTotalPnlRej()" ng-disabled="!selectedDefect">
                                        <h1 style="margin:0px;"><b>1</b></h1>
                                    </a>
                                    <a class="btn btn-lg yellow-gold" ng-click="selectedDefect.NO_OF_DFCT > 99? selectedDefect.NO_OF_DFCT: selectedDefect.NO_OF_DFCT=(selectedDefect.NO_OF_DFCT+'2')*1;setTotalPnlRej()" ng-disabled="!selectedDefect">
